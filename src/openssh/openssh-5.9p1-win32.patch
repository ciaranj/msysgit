+    /*
+     * We pass through to base layer as default.
+     */
+    
+    default:
+    {
+      DBG_MSG("Creating AF_INET socket...");
+      
+      sock = socket(af, type, protocol);
+    }
+  }
+
+  /*
+   * Handle errors.
+   */
+  
+  if (sock == INVALID_SOCKET)
